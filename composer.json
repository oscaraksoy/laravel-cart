{
    "name": "yabhq/laravel-cart",
    "description": "Shopping cart API powered by Laravel",
    "keywords": ["laravel", "cart", "shoppingcart"],
    "homepage": "https://github.com/yabhq/laravel-cart",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jim",
            "email": "jimhlad@gmail.com"
        },
        {
            "name": "Chris",
            "email": "chris@chrisblackwell.me"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0",
        "stripe/stripe-php": "^7.67",
        "yabhq/laravel-mint": "^0.17"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.0.4",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Yab\\ShoppingCart\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yab\\ShoppingCart\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yab\\ShoppingCart\\ShoppingCartServiceProvider"
            ]
        }
    }
}
